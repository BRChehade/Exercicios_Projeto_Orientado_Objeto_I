package Interfaces;

public abstract class Quadrado implements Calcular {
	
	//calcularPermimetro
	//P = L + L + L + L
	//	ou
	//  P = 4L
	
	//calcularArea
	// A = L2

	@Override
	public int calcularCirculoPerimetro(int calcularPerimetroQuadrado) {
		// TODO Auto-generated method stub
		
		return calcularPerimetroQuadrado + calcularPerimetroQuadrado + calcularPerimetroQuadrado +calcularPerimetroQuadrado;
	}

	@Override
	public int calcularCirculoArea(int calcularAreaQuadrado) {
		// TODO Auto-generated method stub
		return calcularAreaQuadrado * calcularAreaQuadrado;
	}

	


	
	

}
