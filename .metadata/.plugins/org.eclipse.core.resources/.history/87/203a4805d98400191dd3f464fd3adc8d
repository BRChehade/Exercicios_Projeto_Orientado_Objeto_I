package Interfaces;

public class Circulo implements CalcularCirculo {


	//calcularPermimetro
	//P = 2 π . r
	

	//calcularArea
	//A = π . r2
		

	@Override
	public int calcularCirculoPerimetro(double raioCirculo) {
		// TODO Auto-generated method stub
		return raioCirculo * (2 * 3.14);
	}

	@Override
	public int calcularCirculoArea(double raioCirculo) {
		// TODO Auto-generated method stub
		return 0;
	}
	



}
